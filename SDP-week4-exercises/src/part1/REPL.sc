package part1

object REPL {
  val myVal1 = 17                                 //> myVal1  : Int = 17
  println(myVal1)                                 //> 17
  val myVal2 = "ABC1234"                          //> myVal2  : String = ABC1234
  println(myVal2)                                 //> ABC1234
  //myVal2 = "DEF1234" will not compile
  val myVal3 = 15.56                              //> myVal3  : Double = 15.56
  println(myVal3)                                 //> 15.56
  
}